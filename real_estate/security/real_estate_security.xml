<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
<odoo noupdate="0">

<record model="ir.module.category" id="base.module_real_estate">
 <field name="name">Real Estate</field>
  <field name="description">User access level Real Estate</field>
  <field name="sequence">15</field>
</record>

    <!--Group forreal estate user -->
    <record id="group_real_estate_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
         <field name="category_id" ref="base.module_real_estate"/>
    </record>

    <!--Group for real estate manager -->
    <record id="group_real_estate_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field eval="[(4, ref('real_estate.group_real_estate_user'))]" name="implied_ids" />
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
         <field name="category_id" ref="base.module_real_estate"/>
    </record>
    



</odoo>